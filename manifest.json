{
  "manifest_version": 3,
  
  "name": "VK Search",
  "version": "2.1",
  "description": "VK'da arama yapmak için sağ tıklama menüsü işlevi",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
	"tabs",
  "storage"
  ],

  "background": {
 "service_worker": "background.js"
},
 
"web_accessible_resources": [{
 "resources": ["context_menu.css"],
 "matches":["*://*/*"]
}]


}
