{
  "manifest_version": 2,
  "name": "VK Search",
  "version": "1.0",
  "description": "VK'da arama yapmak için sağ tıklama menüsü işlevi",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
	"tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
  
}
